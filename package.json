{
  "name": "pride-london-web",
  "version": "0.1.0",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "gatsby": "^1.9.270",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-styled-components": "^2.0.11",
    "grid-styled": "^4.1.0",
    "moment": "^2.22.1",
    "no-scroll": "^2.1.1",
    "polished": "^1.9.2",
    "querystring": "^0.2.0",
    "react-accessible-accordion": "^2.4.0",
    "react-dates": "^16.7.0",
    "react-flip-move": "^3.0.2",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.3.2",
    "react-onclickoutside": "^6.7.1",
    "react-test-renderer": "^16.3.2",
    "styled-components": "^3.2.6",
    "svg-react-loader": "^0.4.5"
  },
  "eslintIgnore": [
    "src/**/*_spec.js"
  ],
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "precommit": "lint-staged",
    "test": "yarn run jest",
    "test:watch": "yarn run jest --watch",
    "deploy:ci": "gatsby build && now ./public -A ../now.json -e NODE_ENV=production --token $NOW_TOKEN --team=prideinlondon && now alias --token $NOW_TOKEN --team=prideinlondon"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add -A"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "gatsby-source-contentful": "^1.3.52",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "jest-styled-components": "^5.0.1",
    "lint-staged": "^7.1.2",
    "now": "^11.1.12",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-markdown": "^3.3.2",
    "react-router-dom": "^4.2.2"
  },
  "jest": {
    "testMatch": [
      "**/?(*_)(spec|test).js"
    ],
    "setupFiles": [
      "./src/configuration/jest.config.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
